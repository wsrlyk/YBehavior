                        <Int Content="Int型的数据"/>
                        <Float Content="Float型的数据"/>
                        <Bool Content="Bool型的数据"/>
                        <Ulong Content="Ulong型的数据"/>
                        <String Content="String型的数据"/>
                </ReadRegister>
                <WriteRegister Content="写入寄存器。写完清空输入数组">
                        <Event Content="事件名，用于区分寄存器中的值的来源"/>
